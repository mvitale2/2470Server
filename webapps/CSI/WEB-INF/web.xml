<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
   <servlet>
     <servlet-name>SimplePostRequest</servlet-name>
     <servlet-class>SimplePostRequest</servlet-class>
   </servlet>

   <servlet-mapping>
     <servlet-name>SimplePostRequest</servlet-name>
     <url-pattern>/SimplePostRequest</url-pattern>
   </servlet-mapping>

   <servlet>
     <servlet-name>EncryptedMessageServlet</servlet-name>
     <servlet-class>EncryptMessageServlet</servlet-class>
   </servlet>

   <servlet-mapping>
     <servlet-name>EncryptedMessageServlet</servlet-name>
     <url-pattern>/EncryptMessage</url-pattern>
   </servlet-mapping>

   <servlet>
     <servlet-name>index</servlet-name>
     <servlet-class>index</servlet-class>
   </servlet>

   <servlet-mapping>
     <servlet-name>index</servlet-name>
     <url-pattern>/index</url-pattern>
   </servlet-mapping>

</web-app>